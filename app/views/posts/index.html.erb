<div id="main">
  <%= image_tag 'main-img.jpg' %>
</div>

<div id="article_wrap">
  <div id="article_list">
    <% @posts.each do |post| %>
     <div>
      <h2><%= link_to post.title, detail_post_path(post) %></h2>
      <p><%= post.created_at.strftime('%Y/%m/%d')  %></p>
     </div>
    <% end %>
 </div>
